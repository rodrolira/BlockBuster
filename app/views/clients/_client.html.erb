<tr>
  <th scope="row"><%= client.id %></th>
  <td><%= client.name %></td>
  <td><%= client.age %></td>
  <td><%= client.movies.last %></td>
  <td><%= link_to "Mostrar cliente", client %></td>
  <td><%= link_to "Editar cliente", edit_client_path(client) %></td>
  <td><%= button_to "Eliminar cliente", client, method: :delete, class:'btn bg-secondary text-primary' %></td>
</tr>
